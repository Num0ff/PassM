<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="200.0" prefWidth="300.0" style="-fx-border-color: #000000; -fx-border-width: 3;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.DeleteController">
   <children>
      <Label alignment="TOP_CENTER" layoutX="20.0" layoutY="29.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="77.0" prefWidth="261.0" text="Вы действительно хотите удалить этот аккаунт?" wrapText="true">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button fx:id="DeleteButton" layoutX="20.0" layoutY="150.0" maxHeight="30.0" maxWidth="100.0" minHeight="30.0" minWidth="100.0" mnemonicParsing="false" onAction="#deleteAccount" prefHeight="30.0" prefWidth="100.0" style="-fx-background-color: #ffffff;" text="Удалить">
         <effect>
            <DropShadow color="#00000080" offsetX="3.0" offsetY="3.0" />
         </effect>
      </Button>
      <Button fx:id="CancelButton" layoutX="180.0" layoutY="150.0" maxHeight="30.0" maxWidth="100.0" minHeight="30.0" minWidth="100.0" mnemonicParsing="false" onAction="#closeWindow" prefHeight="30.0" prefWidth="100.0" style="-fx-background-color: #ffffff;" text="Отмена">
         <effect>
            <DropShadow color="#00000080" offsetX="3.0" offsetY="3.0" />
         </effect>
      </Button>
   </children>
</AnchorPane>
